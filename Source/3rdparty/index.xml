<?xml version="1.0"?>
<libraries
	name = "Third Party Libraries for FT9xx" desc = "v2.7.0">
	<library>
		<name>FatFs</name>
		<desc>Embedded FAT file system library.</desc>
		<dir dest="lib/FatFs" src="FatFs-R0.15" />
	</library>
	<library>
		<name>FreeRTOS</name>
		<desc>Embedded RTOS.</desc>
		<dir dest="lib/FreeRTOS" src="FreeRTOS-v9.0.0" />
	</library>
	<library>
		<name>tinyprintf</name>
		<desc>Very small footprint library providing printf, sprintf etc.</desc>
		<include>lib/tinyprintf</include>
		<file dest="lib/tinyprintf/tinyprintf.c" src="tinyprintf/tinyprintf.c" />
		<file dest="lib/tinyprintf/tinyprintf.h" src="tinyprintf/tinyprintf.h" />
		<file dest="lib/tinyprintf/assert.h" src="tinyprintf/assert.h" />
	</library>
	<library>
		<name>lwIP</name>
		<desc>Lightweight Internet Protocol.</desc>
		<dir dest="lib/lwIP" src="lwIP-2.1.2" />
	</library>
	<library>
		<name>mbedTLS</name>
		<desc>Embedded secure socket implementation.</desc>
		<dir dest="lib/mbedTLS" src="mbedTLS-2.11.0" />
	</library>
 </libraries>